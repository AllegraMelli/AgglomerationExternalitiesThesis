Pulse Function=PULSE ( 10 , 5 )
	~	Dmnl
	~		|

Endgame shock=-Pulse Function*"Government funding for public R&D in sector"*0.8
	~	$
	~		|

Shock endgame=-Pulse Function*Percentage of innovative SMEs collaborating with others\
		*0.8
	~	Dmnl/Year
	~		|

Shock endgame 0[Sectors]=-Pulse Function *"Public Funding for Private R&D in Sector"[\
		Sectors]*0.8
	~	Dmnl
	~		|

"Public-private collaboration per academic"=0.0654
	~	patent/Year/people
	~		|

Absorption Capacity[Sectors,OtherSectors]=(Percentage of innovative SMEs collaborating with others\
		+Shock endgame)*Relatedness[Sectors,OtherSectors
	]*Industry knowledge size[OtherSectors]*(Workforce employed in knowledge intensive activities\
		[OtherSectors]/Workforce employed in knowledge intensive activities
	[Sectors])
	~	patent/Year
	~		|

"knowledge workforce in public R&D"=MIN ( "Workforce wanting to work in academia & public research"\
		, ("Government funding for public R&D in sector"+Endgame shock)
	/"Public R&D expenditure per personel")
	~	people
	~		|

"R&D expenditure in Sector"[Sectors]=("Percentage of R&D budget on total budget in Sector"\
		[Sectors]+Shock endgame 0[Sectors])*(Commercial expenditure
	[Sectors]+External investments)+"Public Funding for Private R&D in Sector"[Sectors]*\
		Commercial expenditure[Sectors]
	~	$/Year
	~		|

Innovation expenditure per person employed in innovation in Sector[Sectors]="R&D expenditure in Sector"\
		[Sectors]/Workforce employed in knowledge intensive activities[Sectors]
	~	$/(people*Year)
	~		|

Years in fertile age=SUM ( Average time per age group[FertileYears!])
	~	Year
	~		|

FertileYears:Age18to24, Age25to45
	~	
	~		|

Ageing[AgeGroups]=Population[AgeGroups]/Average time per age group[AgeGroups]
	~	people/Year
	~		|

Initial population[AgeGroups]=736353, 261077, 2335.64, 1.2761e+06, 2000, 2000, 1965, \
		680791
	~	people
	~	http://statline.cbs.nl/Statweb/publication/?DM=SLNL&PA=7461BEV
	|

Population[Age0to17]= INTEG (Births-Ageing[Age0to17]-Deaths[Age0to17],
		Initial population[Age0to17]) ~~|
Population[Age18to24]= INTEG (Ageing[Age0to17]+Deaths[Age18to24]-Ageing[Age18to24]-Ageing\
		[Age0to17]*Percentage of adults enrolling in studies,
		Initial population[Age18to24]) ~~|
Population[Students]= INTEG (Deaths[Students]-Ageing[Students]+Ageing[Age0to17]*Percentage of adults enrolling in studies\
		,
		Initial population[Students]) ~~|
Population[Age25to45]= INTEG (Ageing[Age18to24]-Deaths[Age25to45]-Ageing[Age25to45],
		Initial population[Age25to45]) ~~|
Population[Graduates25to45]= INTEG (Ageing[Students]-Ageing[Graduates25to45]-Deaths[Graduates25to45\
		],
		Initial population[Graduates25to45]) ~~|
Population[Age45to65]= INTEG (Ageing[Age25to45]-Deaths[Age45to65]-Ageing[Age45to65],
		Initial population[Age45to65]) ~~|
Population[Graduates45to65]= INTEG (Ageing[Graduates25to45]-Deaths[Graduates45to65]-Ageing\
		[Graduates45to65],
		Initial population[Graduates45to65]) ~~|
Population[Age65toLifeExpectancy]= INTEG (Ageing[Age45to65]-Deaths[Age65toLifeExpectancy\
		]+Ageing[Graduates45to65]-Ageing[Age65toLifeExpectancy],
		Initial population[Age65toLifeExpectancy])
	~	people
	~	Ageing[PreviousAgeGroups]-Ageing[AllButYoungestAndOldest]-Deaths[AllButYoun\
		gestAndOldest]
	|

"Demand for private R&D workforce in sector"[Sectors]="Percentage of workforce wanting to go to the private R&D"\
		*(Population[Graduates25to45]+Population[Graduates45to65])*Industry attractiveness[\
		Sectors]
	~	people
	~		|

Births=Average fertility per woman * Relative share women in fertile age * SUM(Population\
		[FertileAge!])/Years in fertile age
	~	people/Year
	~		|

Deaths[AgeGroups]=Population[AgeGroups] * Average mortality per age[AgeGroups]
	~	people/Year
	~		|

"Workforce wanting to work in academia & public research"="Percentage of workforce wanting to work in public R&D"\
		*(Population[Graduates25to45]+Population[Graduates45to65])
	~	people
	~		|

