Knowledge Spillover[Sectors]=("Public-private collaboration per academic"[Sectors]+Struggle synergies shock 0\
		[Sectors])*"knowledge workforce in public R&D"[Sectors
	]+"Inter-sector knowledge spillover"[Sectors]
	~	patent/Year
	~		|

Struggle synergies shock 0[Sectors]=PULSE ( 10 , 5)*"Public-private collaboration per academic"\
		[Sectors]*0.8*SIN(Time*3.14159)
	~	Dmnl/Year
	~		|

"Public-private collaboration per academic"[Sectors]=0.09
	~	patent/Year/people
	~		|

Absorption Capacity[Sectors,OtherSectors]=(Percentage of innovative SMEs collaborating with others\
		+Struggle synergies shock)*Relatedness[Sectors,OtherSectors
	]*Industry knowledge size[OtherSectors]*(Workforce employed in knowledge intensive activities\
		[OtherSectors]/Workforce employed in knowledge intensive activities
	[Sectors])
	~	patent/Year
	~		|

Struggle synergies shock=PULSE ( 10 , 5)*Percentage of innovative SMEs collaborating with others\
		*0.8*SIN(Time*3.14159)
	~	Dmnl/Year
	~		|

"R&D expenditure in Sector"[Sectors]=("Percentage of R&D budget on total budget in Sector"\
		[Sectors]+Struggle synergies shock 1[Sectors])/100*(Commercial expenditure
	[Sectors]+External investments)+"Public Funding for Private R&D in Sector"[Sectors]/\
		100*Commercial expenditure[Sectors]
	~	$/Year
	~		|

